<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_contact_choose"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.haiqiu.miaohi.widget.pulltorefreshview.PullToRefreshStickyHeaderListView
        android:id="@+id/pull_to_refresh_stickyheader_listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:scrollbars="none" />


    <com.haiqiu.miaohi.widget.LetterSelectorView
        android:id="@+id/mLetterListView"
        android:layout_width="25dp"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        android:background="#00000000"
        android:clickable="true" />
</RelativeLayout>
